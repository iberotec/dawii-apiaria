<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <!-- ManageBean -->
 <managed-bean>
  <managed-bean-name>mBUsuarioApiarioExtraccion</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBUsuarioApiarioExtraccion</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <!-- Diego -->
 <managed-bean>
  <managed-bean-name>mBPlanillaRevision</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBPlanillaRevision</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBSelectItems</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.commons.MBSelectItems</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBApiario</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBApiario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBLogueo</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBLogueo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBEstadoRevisionEquipoTrabajo</managed-bean-name>
