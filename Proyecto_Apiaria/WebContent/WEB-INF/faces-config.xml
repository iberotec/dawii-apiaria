  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBMantenimientoApiario</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBMantenimientoApiario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBMantenimientoColmena</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBMantenimientoColmena</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBMantenimientoPiso</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBMantenimientoPiso</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBMantenimientoAlza</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBMantenimientoAlza</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBCosecha</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBCosecha</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>normaSeguridadConverter</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.util.NormaSeguridadConverter</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>planillaCosechaAlzaConverter</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.util.PlanillaCosechaAlzaConverter</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <!-- navegacion -->
 <navigation-rule>
  <navigation-case>
   <from-outcome>successBienvenida</from-outcome>
   <to-view-id>/UI_bienvenida.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoUsuarioApiario</from-outcome>
   <to-view-id>/seleccion/UI_mantenimientoUsuarioApiario.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoUsuarioApiarioExtraccion</from-outcome>
   <to-view-id>/extraccion/UI_mantenimientoUsuarioApiarioExtraccion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevoUsuarioApiarioExtraccion</from-outcome>
   <to-view-id>/extraccion/UI_registrarUsuarioApiarioExtraccion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successLogin</from-outcome>
   <to-view-id>/UI_login.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevaAsignacion</from-outcome>
   <to-view-id>/seleccion/UI_registrarUsuarioApiario.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMainCosecha</from-outcome>
   <to-view-id>/cosecha/mainCosecha.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevaAsignacionCosecha</from-outcome>
   <to-view-id>/cosecha/seleccionarCosecha/nuevaExtraccion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMainCosechaa</from-outcome>
   <to-view-id>/cosecha/UI_mantenimientoUsuarioDecantadoraCentrifugadora.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevaAsignacionCosechaa</from-outcome>
   <to-view-id>/cosecha/UI_registrarUsuarioDecantadoraCentrifugadora.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoApiario</from-outcome>
   <to-view-id>/tablas/apiario/mantenimientoApiario.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevoApiario</from-outcome>
   <to-view-id>/tablas/apiario/nuevoApiario.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoColmena</from-outcome>
   <to-view-id>/tablas/colmena/mantenimientoColmena.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevoColmena</from-outcome>
   <to-view-id>/tablas/colmena/nuevoColmena.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoPiso</from-outcome>
   <to-view-id>/tablas/piso/mantenimientoPiso.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevoPiso</from-outcome>
   <to-view-id>/tablas/piso/nuevoPiso.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMantenimientoAlza</from-outcome>
   <to-view-id>/tablas/alza/mantenimientoAlza.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successNuevoAlza</from-outcome>
   <to-view-id>/tablas/alza/nuevoAlza.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!--navegacion -->
 <application>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
 </application>
</faces-config>
