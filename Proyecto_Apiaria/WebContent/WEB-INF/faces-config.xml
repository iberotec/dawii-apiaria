<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <!-- ManageBean -->
 <managed-bean>
  <managed-bean-name>mBapiaria</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBVerficaApiaria</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBNormaSeguridad</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBNormaSeguridad</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>mBApiariaAsignada</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBApiarioAsignado</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>mBPlanillaRevision</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBPlanillaRevision</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <!-- Diego -->
 <managed-bean>
  <managed-bean-name>mBSelectItems</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.commons.MBSelectItems</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBApiario</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBApiario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>mBLogueo</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBLogueo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>mBEstadoRevisionEquipoTrabajo</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBDetalleEquipamientoTrabajo</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
  <managed-bean>
  <managed-bean-name>mBPlanillaRevisionAlza</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBPlanillaRevisionAlza</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean> 
   <managed-bean>
  <managed-bean-name>mBNormaSeguridadApiario</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBNormaSeguridadApiario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean> 
  <managed-bean>
  <managed-bean-name>mBUsuarioApiario</managed-bean-name>
  <managed-bean-class>apiario.edu.pe.managedBeans.MBUsuarioApiario</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean> 
 <!-- navegacion -->
 <navigation-rule>
  <navigation-case>
   <from-outcome>successBienvenida</from-outcome>
   <to-view-id>/UI_bienvenida.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMainSeleccion</from-outcome>
   <to-view-id>/seleccion/mainSeleccion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successLogin</from-outcome>
   <to-view-id>/UI_login.xhtml</to-view-id>
  </navigation-case>
 <navigation-case>
   <from-outcome>successUbicacion</from-outcome>
   <to-view-id>/extraccion/mainExtraccion.xhtml</to-view-id>
  </navigation-case>
   <navigation-case>
   <from-outcome>successNuevaAsignacion</from-outcome>
   <to-view-id>/seleccion/temporadaSeleccion/nuevaAsignacion.xhtml</to-view-id>
  </navigation-case>
   <navigation-case>
   <from-outcome>successMainCosecha</from-outcome>
   <to-view-id>/cosecha/mainCosecha.xhtml</to-view-id>
  </navigation-case>
   <navigation-case>
   <from-outcome>successNuevaAsignacionCosecha</from-outcome>
   <to-view-id>/cosecha/seleccionarCosecha/nuevaExtraccion.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <!--navegacion -->
 
 
 
 
 
 <application>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
 </application>
</faces-config>
