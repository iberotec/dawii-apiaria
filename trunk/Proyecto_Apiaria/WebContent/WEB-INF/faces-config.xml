 ManageBean 
navegacion -->
 <navigation-rule>
  <!-- Diego -->
  <navigation-case>
   <from-outcome>successBienvenida</from-outcome>
   <to-view-id>/UI_bienvenida.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successMainSeleccion</from-outcome>
   <to-view-id>/seleccion/mainSeleccion.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>successLogin</from-outcome>
   <to-view-id>/UI_login.xhtml</to-view-id>
  </navigation-case>
  <!-- Diego -->
 </navigation-rule>
 <!--navegacion -->
 <application>
  <locale-config>
   <default-locale>es</default-locale>
   <supported-locale>es</supported-locale>
   <supported-locale>en</supported-locale>
  </locale-config>
 </application>
</faces-config>
